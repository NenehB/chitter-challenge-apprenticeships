<html>
  <head>
    <meta charset="utf-8">
    <title> Chitter App</title>
  </head>
  
  <body>
  <table class="table">
  <tr>
    <th scope="col">Peeps</th>
   </tr>
  <% @peeps.each do |peep| %>
    <tr>
      <td><%= peep %></td>
      </tr>
  <% end %>
</table>


<h3>Add a message</h3>
<form action="/messages" method="post">
<textarea name="textarea" rows="4" cols="50" maxlength="50">
Enter text here...</textarea>
<input type="Submit" name="submit>"
</form>

  </body>
</html>